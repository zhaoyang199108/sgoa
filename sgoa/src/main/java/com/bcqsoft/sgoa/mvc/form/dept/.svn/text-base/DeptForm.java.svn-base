package com.bcqsoft.sgoa.mvc.form.dept;

import java.util.Date;

import com.bcqsoft.sgoa.core.base.BaseForm;

public class DeptForm extends BaseForm {

	/**
	 * 自增型主键
	 */
	private Long id;

	/**
	 * 创建日期
	 */
	private Date createDate;

	/**
	 * 更新日期
	 */
	private Date updateDate;

	/**
	 * 部门名称
	 */
	private String deptName;

	/**
	 * 所属单位ID(关联SCOA_TB_UNIT.ID)
	 */
	private Long unitId;

	/**
	 * 部门职责
	 */
	private String deptDuty;

	/**
	 * 部门电话: 为内勤电话(当某部门设置内勤时更新部门电话)
	 */
	private String deptTell;

	/**
	 * 负责人ID: 为内勤ID(当某部门设置内勤时更新负责人ID), 关联SCOA_TB_USER.LOGIN_ID
	 */
	private String adminId;

	/**
	 * 是否是有效记录(Y:有效, N:无效)
	 */
	private String enabled;
	/**
	 * 负责人姓名
	 */
	private String adminName;


	/**
	 * @return the adminName
	 */
	public String getAdminName() {
		return adminName;
	}

	/**
	 * @param adminName the adminName to set
	 */
	public void setAdminName(String adminName) {
		this.adminName = adminName;
	}

	/**
	 * 取得自增型主键
	 */
	public Long getId() {
		return id;
	}

	/**
	 * 设置自增型主键
	 */
	public void setId(Long id) {
		this.id = id;
	}

	/**
	 * 取得创建日期
	 */
	public Date getCreateDate() {
		return createDate;
	}

	/**
	 * 设置创建日期
	 */
	public void setCreateDate(Date createDate) {
		this.createDate = createDate;
	}

	/**
	 * 取得更新日期
	 */
	public Date getUpdateDate() {
		return updateDate;
	}

	/**
	 * 设置更新日期
	 */
	public void setUpdateDate(Date updateDate) {
		this.updateDate = updateDate;
	}

	/**
	 * 取得部门名称
	 */
	public String getDeptName() {
		return deptName;
	}

	/**
	 * 设置部门名称
	 */
	public void setDeptName(String deptName) {
		this.deptName = deptName;
	}

	/**
	 * 取得所属单位ID(关联SCOA_TB_UNIT.ID)
	 */
	public Long getUnitId() {
		return unitId;
	}

	/**
	 * 设置所属单位ID(关联SCOA_TB_UNIT.ID)
	 */
	public void setUnitId(Long unitId) {
		this.unitId = unitId;
	}

	/**
	 * 取得部门职责
	 */
	public String getDeptDuty() {
		return deptDuty;
	}

	/**
	 * 设置部门职责
	 */
	public void setDeptDuty(String deptDuty) {
		this.deptDuty = deptDuty;
	}

	/**
	 * 取得部门电话: 为内勤电话(当某部门设置内勤时更新部门电话)
	 */
	public String getDeptTell() {
		return deptTell;
	}

	/**
	 * 设置部门电话: 为内勤电话(当某部门设置内勤时更新部门电话)
	 */
	public void setDeptTell(String deptTell) {
		this.deptTell = deptTell;
	}

	/**
	 * 取得负责人ID: 为内勤ID(当某部门设置内勤时更新负责人ID), 关联SCOA_TB_USER.LOGIN_ID
	 */
	public String getAdminId() {
		return adminId;
	}

	/**
	 * 设置负责人ID: 为内勤ID(当某部门设置内勤时更新负责人ID), 关联SCOA_TB_USER.LOGIN_ID
	 */
	public void setAdminId(String adminId) {
		this.adminId = adminId;
	}

	/**
	 * 取得是否是有效记录(Y:有效, N:无效)
	 */
	public String getEnabled() {
		return enabled;
	}

	/**
	 * 设置是否是有效记录(Y:有效, N:无效)
	 */
	public void setEnabled(String enabled) {
		this.enabled = enabled;
	}
}

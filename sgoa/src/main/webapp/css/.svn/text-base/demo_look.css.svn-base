h1, h2, h3, h4, h5, h6 {color: #2f332a;font-weight: bold;font-family: Helvetica, Arial, sans-serif;padding-bottom: 5px;}
h1 {font-size: 24px;line-height: 34px;text-align: center;}
h2 {font-size: 14px;line-height: 24px;padding-top: 5px;}
h6 {font-weight: normal;font-size: 12px;letter-spacing: 1px;line-height: 24px;text-align: center;}
input.radio {margin: 0 2px 0 8px;}
input.radio.first {margin-left:0;}
input.empty {color: lightgray;}
code {color: #2f332a;}
.highlight_red {color:#A60000;}
.highlight_green {color:#A7F43D;}
li {font-size: 12px;}
li.title {list-style: none;}
ul.list {margin-left: 17px;}

div.content_wrap {width:1150px!important;width:1000px;height:380px;}
div.content_wrap div.left{float: left;width:350px;}
div.content_wrap div.right{float: right;width: 620px;}
div.zTreeDemoBackground {width:350px;height:368px;text-align:left;}
div.user_list{margin-top:10px;border: 1px solid #617775;width:610px;height:370px;overflow-y:scroll;overflow-x:hidden;}
table.kow{ width:610px; border:1px solid #B0C8B5; border-collapse: collapse; border-spacing:0; text-align:center; background:#FFFFFF}
table.kow td{ line-height:23px; height:23px; font-size:14px; font-weight:normal; border:1px solid #B0C8B5;}
table.kow th{ line-height:23px; height:23px; font-size:14px; font-weight:normal; background:#CDE4CE; border:1px solid #B0C8B5;}

ul.ztree{margin:10px 0px 0px 0px; border: 1px solid #617775;background:#E7F1EC;height:360px;overflow-y:scroll;overflow-x:auto;}
ul.log {border: 1px solid #617775;background: #f0f6e4;width:300px;height:170px;overflow: hidden;}
ul.log.small {height:45px;}
ul.log li {color: #666666;list-style: none;padding-left: 10px;}
ul.log li.dark {background-color: #E3E3E3;}

/* ruler */
div.ruler {height:20px; width:220px; background-color:#f0f6e4;border: 1px solid #333; margin-bottom: 5px; cursor: pointer}
div.ruler div.cursor {height:20px; width:30px; background-color:#3C6E31; color:white; text-align: right; padding-right: 5px; cursor: pointer}

.btn-bk a {
    float: left;
    width: 59px;
    height: 40px;
    background: url(../image/btn.jpg) no-repeat;
    text-indent: -2000em;
}

.btn-bk a:hover {
    background: url(../image/btn_hover.jpg) no-repeat 0 0;
}

.btn-bk a.add-btn {
    background-position: 0 0;
}
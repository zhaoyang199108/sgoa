#jsddm li a
	{	display: block;
	font-size:14px; background: url(../images/mue.jpg); font-weight:bold; line-height:36px; width:105px; height:36px; display:block; color:#FFFFFF;	}
#jsddm li a:hover
	{	background: url(../images/menu_hou.jpg); color:#FF9900;}
		
#jsddm li ul.xl{margin-top: 2px; padding: 0px;position: absolute;  visibility: hidden; padding-left:17px; _width:580px;}
#jsddm li ul.xl li{	float: none;
				display: inline; margin:0px; padding:0px; float:left; }
			
#jsddm li ul.xl li a{	width:78px;filter:alpha(opacity=95); opacity: 0.95; margin:1px 3px;float:left;
				background:#31A8EC; height:30px; line-height:30px;
				color:#ffffff; font-size:12px; text-align:center; border: 1px solid #0293D9; filter:alpha(opacity=80);

/*首页部分*/
.chromemenu{
width: 74%; margin-left:3%; display:inline; float:left; 
}
.chromemenu:after{ /*Add margin between menu and rest of content in Firefox*/
content: "."; 
display: block; 
height: 0; 
clear: both; 
visibility: hidden;
}
.chromemenu ul{
height:41px; text-align:center;/*set value to "right" for example to align menu to the left of page*/
}
.chromemenu ul li{
height:40px; float:left; padding-top:3px; padding-right:2%; padding-left:6px;
}
.chromemenu ul li:hover a{background: url(../images/menu_hou.jpg); color:#FF9900;}
.chromemenu ul li a{
	display: block;
	font-size:14px; background: url(../images/mue.jpg); font-weight:bold; line-height:36px; width:105px; height:36px; display:block; color:#FFFFFF;
}

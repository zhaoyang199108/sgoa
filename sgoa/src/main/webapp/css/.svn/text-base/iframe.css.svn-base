@charset "utf-8";
body, div, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, input, button, textarea, p, blockquote, th, td a {
    margin: 0;
    padding: 0;
}
table {
    border-collapse: collapse;
    border-spacing: 0;
}
img {
    border: 0;
}
li {
    list-style: none;
}
h1, h2, h3, h4, h5, h6 {
    font-weight: normal;
}
body {
    font: 12px / 22px "宋体";
    color:#333333; 
    text-align: center; background:#F8F8F8;
}
a {
    color: #333333;
    text-decoration: none;
}

a:hover {
    color: #333333;
    text-decoration: none;
}
/*增加页页样式*/
table.ftb{ width:85%; margin:20px auto; background:#FBFDFF;}
table.ftb th{ background:#E1F0FE; padding:2px 6px; border:1px solid #C9E4FE; width:19%; text-align:left;}
table.ftb th h3.dzb{ font-size:16px; font-weight:bold; color:#000000; line-height:26px;}
table.ftb td{border:1px solid  #C9E4FE; padding:4px 6px; text-align:left;}
table.ftb td.cc{height:390px;float:left; padding-right:18px;}
table.ftb td input.title{ width:80%; height:22px; line-height:22px;}
table.ftb td input{ padding-left:3px; }
table.ftb td input.iowoe{ width:58%; _width:58%; padding-left:3px; }

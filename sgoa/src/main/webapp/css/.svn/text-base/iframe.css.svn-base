table.st tr.act td{text-align:right; padding-right:50px;}


.pstty { color:#000000; width:98%; margin:0 auto;
}
.pstty h4{ text-align:center; font-size:24px; font-weight:bold; line-height:50px;}
.pstty table.stty{ font-size:14px; width:100%;}
.pstty table.stty td{ *width:14%; border:1px solid #C9E4FE; line-height:28px; padding:3px; text-align:left;}

table.stty td input.btn02{background:url(../images/pic-bg.png) no-repeat -7px -582px; border:1px solid #3188F2; width:70px; height:26px; border:0px; padding:0px 5px; float:right; margin-right:50px;}
table.stty td input.btnnb{width:100px; border:2px solid #95C3E6; background:#D6EAF5; height:24px;}
table.stty td input.btnnb02{width:100px; border:1px solid #E0DCCE; background:#FDFCF7; height:24px; color:#353535; border-radius:4px; cursor:pointer;}

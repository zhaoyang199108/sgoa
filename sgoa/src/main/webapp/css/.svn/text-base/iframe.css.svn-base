.pst { color:#FF0000; width:98%; margin:0 auto;
}
.pst h4{ text-align:center; font-size:28px; font-weight:bold; line-height:70px;}
.pst table.st{ font-size:14px; width:100%;}
.pst table.st td{ *width:23%; border:1px solid #FF0000; line-height:28px; padding:3px; text-align:left;}
.pst table.st td.rre input{ border:0px; width:16%;}
.pst table.st td input{ width:80%; border:1px solid #D8D8D8; }

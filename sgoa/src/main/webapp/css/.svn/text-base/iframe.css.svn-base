table.ftb td input.btn{ background:url(../images/pic-bg.png) no-repeat -7px -582px; border:1px solid #3188F2; width:70px; height:26px; border:0px; padding:0px 5px;}
table.ftb td input.btn:hover{ background-position:-108px -582px; cursor:hand;}
table.ftb td input.txt title{  padding-left:3px; }
table.ftb tr.act td{ text-align:right;}
.woe{ margin:0px; padding:5px; width:78%; height:60px;}
